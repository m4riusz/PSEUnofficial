{
    "targets": {
        "App-iOS": {
            "platform": "iOS",
            "type": "application",
            "sources": [
                {
                    "path": "Sources"
                },
                {
                    "path": "Info.plist"
                }
            ],
            "dependencies": [
                {
                    "target": "Core"
                }
            ]
        },
        "App-iOSTests": {
            "platform": "iOS",
            "type": "bundle.unit-test",
            "settings": {
                "TEST_HOST": "$(BUILT_PRODUCTS_DIR)/App-iOS.app/App-iOS"
            },
            "sources": [
                {
                    "path": "Tests"
                }
            ],
            "dependencies": [
                {
                    "target": "App-iOS"
                }
            ]
        },
        "App-iOSUITests": {
            "platform": "iOS",
            "type": "bundle.ui-testing",
            "settings": {
                "TEST_TARGET_NAME": "App-iOS"
            },
            "sources": [
                {
                    "path": "UITests"
                }
            ],
            "dependencies": [
                {
                    "target": "App-iOS"
                }
            ]
        }
    }
}