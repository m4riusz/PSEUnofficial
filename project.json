{
	"name": "PSEUnofficial",
	"options": {
		"bundleIdPrefix": "pl.msut",
		"createIntermediateGroups": true,
		"generateEmptyDirectories": true,
		"groupSortPosition": "top",
		"groupOrdering": [
			{
				"order": ["Sources", "Resources", "Tests", "UITests"],
				"pattern": ".*"
			}
		]
	},
	"include": [
		"templates.json",
		"packages.json",
		"Core/Core.json",
		"iOS/iOS.json"
	],
	"schemes": {
		"iOS": {
			"build": {
				"targets": {
					"iOS": "all"
				}
			},
			"test": {
				"gatherCoverageData": true,
				"language": "en",
				"region": "EN_US",
				"coverageTargets": [
					"Core",
					"iOS"
				],
				"targets": [
					"CoreTests",
					"iOSTests"
				]
			}
		},
		"iOSUITests": {
			"build": {
				"targets": {
					"iOSUITests": [
						"run",
						"test"
					]
				}
			},
			"test": {
				"targets": [
					"iOSUITests"
				]
			}
		}
	}
}
